{
  "appId": "io.ionic.starter",
  "appName": "rehapp",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "androidSplashResourceName": "screen",
      "showSpinner": true
    },
    "GoogleAuth": {
      "scopes": ["profile", "email"],
      "serverClientId": "DO_NOT_COMMIT",
      "forceCodeForRefreshToken" : true
    }
  },
  "cordova": {}
}
